  "description": "a simple library to display a tree such as in a file explorer",
  "main": "dist/bundle.js",
  "scripts": {
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "build": "rollup -c && shx cp dist/bundle.js example/treexplorer.js"
